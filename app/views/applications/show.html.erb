<%= @application.name %>
<%= @application.address %>
<%= @application.city %>
<%= @application.state %>
<%= @application.zip %>
<%= @application.phone_number %>
<%= @application.description %>

<%= @application.pets.each do |pet| %>
<%= link_to "#{pet.name}", "/pets/#{pet.id}" %>
<%= button_to "Approve Application For #{pet.name}", "/applications/#{@application.id}/approvals?pet_id=#{pet.id}"%>
<% end %>
